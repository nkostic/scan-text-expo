# OCR.space API Configuration
# Sign up for a free API key at https://ocr.space/ocrapi
# Free tier: 25,000 requests/month
OCR_API_KEY=your_ocr_space_api_key_here

# Optional: Enable debug logging
DEBUG=false

# Optional: OCR Engine (1 or 2)
# Engine 1: Faster, Engine 2: More accurate
OCR_ENGINE=2

# Optional: OCR Language
# Supported: eng, spa, fra, deu, por, rus, ara, hin, jpn, kor, chi_sim, chi_tra
OCR_LANGUAGE=eng
